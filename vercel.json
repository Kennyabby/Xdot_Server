{
  "version": 2,
  "name": "encarto-server",
  "builds": [{ "src": "index.js", "use": "@vercel/node" }],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js",
      "headers": {
        "Access-Control-Allow-Origin": "http://xdot.vercel.app, https://xdot.vercel.app, http://localhost:3000"
      }
    }
  ]
}
